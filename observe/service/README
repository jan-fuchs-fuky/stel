*** testy ***

$ ssh -L 7003:localhost:7003 -L 7004:localhost:7004 primula
$ ssh -L 6000:localhost:6000 -L 6001:localhost:6001 -L 7001:localhost:7001 -L 7002:localhost:7002 tcsuser@pc-tubus

*** pc-tubus ***

$ cd /home/tcsuser/sds/bin
$ ./sds_client.py -d

$ cd /home/tcsuser/_tmp/dev/service/bin
$ ./fiber_gxccd_server.py -n pointing
$ ./fiber_gxccd_server.py -n photometric

$ cd /home/tcsuser/_tmp/service/bin
$ ./fiber_control_server.py

INSTALL:

    # cd /opt
    # mkdir sds fiber_gxccd_server fiber_control_server

    # mkdir /opt/fiber_gxccd_server/fits_idx/
    # chown tcsuser.tcsuser /opt/fiber_gxccd_server/fits_idx/

    # vim /etc/udev/rules.d/10-local.rules
    ATTR{idVendor}=="1347", ATTR{idProduct}=="0412", MODE="0600", OWNER="tcsuser"
    ATTR{idVendor}=="1347", ATTR{idProduct}=="0406", MODE="0600", OWNER="tcsuser"
    # G2-1000BI
    ATTR{idVendor}=="1347", ATTR{idProduct}=="04b1", MODE="0600", OWNER="tcsuser"

    # udevadm control --reload-rules
    # udevadm trigger

    # usermod -a -G dialout tcsuser

    # vim /etc/sudoers.d/tcsuser
    tcsuser    ALL = NOPASSWD: /opt/bin/usb_reattach

